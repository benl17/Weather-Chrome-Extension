{
    "name": "Weather & Clothes Suggestion Chrome Extension", 
    "version": "1.0",
    "description": "A chrome extension that displays the weather as well as what clothes you should probably wear to be most comfortable", 
    "manifest_version": 3, 
    "permissions": [
        "geolocation"
    ],
    "icons": {
        "16": "weather_app_icon1.png",
        "32": "weather_app_icon1.png",
        "48": "weather_app_icon1.png",
        "64": "weather_app_icon1.png",
        "128": "weather_app_icon1.png"
    }, 
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "64": "weather_app_icon1.png",
            "128": "weather_app_icon1.png"
        }
    }
}